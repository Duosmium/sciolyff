{
  "name": "sciolyff",
  "version": "1.0.0",
  "description": "A file format for Science Olympiad tournament results.",
  "license": "MIT",
  "author": "Duosmium",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "sciolyff-js": "dist/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Duosmium/sciolyff-js.git"
  },
  "homepage": "https://github.com/Duosmium/sciolyff-js#readme",
  "bugs": {
    "url": "https://github.com/Duosmium/sciolyff-js/issues"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "format": "prettier --check .",
    "fix": "prettier --write . && eslint --fix .",
    "build": "tsc",
    "prepare": "husky install"
  },
  "keywords": [
    "sciolyff",
    "scioly",
    "science olympiad",
    "duosmium"
  ],
  "dependencies": {
    "commander": "^8.0.0",
    "fetch-retry": "^4.1.1",
    "js-yaml": "^4.1.0",
    "neat-csv": "^6.0.1",
    "node-fetch": "^2.6.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.2",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,md}": "prettier --write",
    "*.{js,ts}": "eslint --cache --fix"
  }
}
